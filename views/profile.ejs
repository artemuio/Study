<!DOCTYPE HTML>
<html>
<head>
<title>Profile | <%=user.username%></title>
<link href="/css/style-pr.css" rel="stylesheet" type="text/css" media="all" />
<link href="/images/favicon.ico" rel="icon" type="image/x-icon" />
<script type="text/javascript" src="/js/jquery-1.11.3.js"></script>
<script type="text/javascript" src="/js/profile.js"></script>
</head>
<body>
	<div class="logo">
		<a href="#">
				<img src="/images/logo-pr.gif" alt="travel html5 template" title="travel html5 template">
		</a>
	</div>
<div class="wrap">
	<!-- start grids_of_3 -->
	<div class="grids_of_3">
		<div class="grid1_of_3">
			<div class="grid_img">
				<img src="/images/pic1.jpg" alt=""/>
						<div class="list_top">
							  <h4 id="username"><%=user.username%></h4>
						</div>
			</div>
			<div class="menu_box_list">
				<section class="ac-container">
					<div>
						<input id="ac-1" name="accordion-1" type="checkbox" />
						<label id="new-project" for="ac-1" class="grid1"><i></i><a id="createnewproject" href="#">Create New Project</a></label>
					</div>
					<%table_names.forEach(function(table_names){ %>
					<div>
						<input id="<%=table_names.name%>" name="accordion-1" type="checkbox" />
						<label for="<%=table_names.name%>" class="grid2"><i></i><%=table_names.name%></label>
						<article class="ac-small">
							<ul>
								
									<li><a href="#">asdasd</a></li>
  								
							</ul>
						</article>
					</div>
					<% }); %>
					<div>
						<input id="ac-3" name="accordion-1" type="checkbox" />
						<label for="ac-3" class="grid3"><i></i>STUDY</label>
						<article class="ac-small">
							<ul>
								<li><a href="#">History</a></li>
								<li><a href="#">English</a></li>
							</ul>
						</article>
					</div>
									</section>
				<div class="clear"></div>
			</div>
		</div>
		<div class="grid1_of_3 left main">
			<div class="btn_style bg">
				<form class="searchform">
					 <div class="search">
						<input type="search" name="q">
						<input type="submit" value="">
					</div>
 				</form>
				<div class="top-nav">

						<ul class="menu2">
							<li>
								<div  class="nav-icon"> </div>
								<ul>
									<li><a class="root-nav1" href="/logout">LOGOUT</a></li>
									<li><a id="settings" href="#" class="root-nav2">SETTING PROFILE</a></li>
								</ul>
							</li>
						</ul>
				</div>

				<div class='name'>

					<h4>MY PROFILE</h4>
				</div>

			</div>
			<div class="friends_form">

			</div>
			<div class="login_form">
			</div>
			<div class="news_form">

			</div>
			<div class="footer">
				<p class="link">© All rights reserved | Template by&nbsp;<a href="#"> TUNATEAM</a></p>
			</div>
		</div>
	</div>
</div>
<div id="hover"></div>
<div id="popup">
    	<div id="close">X</div>
			<div class="contact-text2">
			 <form>
				 <div class="image">
					 <a href="#single.html">
					 <img src="images/camera.png" alt="">
					 <p>Загрузить фотографию</p>
					 </a>
				 </div>
						 <input placeholder="Полное имя" class="mail" type="text" required="">
						 <input placeholder="Логин" class="mail" type="text" required="">
						 <input placeholder="E-mail" class="mail" type="text" required="">
						 <div >
				 			<input class="submit2" type="submit" value="Сохранить изменения">
				 		</div>
			 </form>
		 </div>
</div>
<div id="popupforproject">
    <div id="closenewproject">X</div>
	<div class="new_form">
		<div class="Regisration">
			<div class="contact-text2"><h3>Новый проект</h3>
 				<form>
				<div class="image">
					<a href="#single.html">
						<img src="images/camera.png" alt="">
						<p>Логотип проекта</p>
					</a>
				</div>
 				<input id="newprojectname" placeholder="Название проекта" class="mail" type="text" required="">
				<p><select id="selectthemenewproject" name="num" form="data">
			    	<option value="0">Дизайн</option>
			    	<option value="1">Путешествия</option>
			   		<option value="2">Бизнес</option>
					<option value="3">Стартап</option>
					<option value="4">Обучение</option>
					<option value="5">Мероприятие</option>
  				</select></p>
				<div >
					<input class="submit3" type="submit" value=" Добавить участников">
				</div>
					<p><select id="selectformatnewproject" name="num" form="data">
						<option value="0">Открытый</option>
						<option value="1">Закрытый</option>
					</select></p>
 					<textarea id="newprojecttextabout" class="textforadditions" placeholder="Описание проекта"></textarea>
 				</form>
 			</div>
		</div>
		<div >
			<input class="submit2" id="submitbuttonnewproject" type="submit" value="Добавить проект">
		</div>
	</div>
</div>
</body>
</html>
